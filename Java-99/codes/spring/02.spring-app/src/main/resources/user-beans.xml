<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
					
	<bean id="user1" class="co.edureka.spring.core.beans.User" lazy-init="true" scope="prototype">
		<property name="firstName" value="Pankaj"/>
		<property name="lastName" value="Kumar"/>
	</bean>
	
	<bean id="user2" class="co.edureka.spring.core.beans.User">
		<constructor-arg value="Sachin"/>
		<constructor-arg value="Tendulkar"/>
	</bean>
</beans>
