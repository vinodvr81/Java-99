<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="bmw" class="co.edureka.spring.core.beans.Car">
		<property name="model" value="B M W"/>
		<property name="cost" value="5700000"/>
	</bean>

	<bean id="sanjay" class="co.edureka.spring.core.beans.Customer">
		<property name="customerName" value="Sanjay Patil"/>
		<property name="car" ref="bmw"/>
	</bean>
</beans>
