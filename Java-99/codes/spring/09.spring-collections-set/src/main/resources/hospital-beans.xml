<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="apollo" class="co.edureka.spring.core.beans.Hospital">
		<property name="hospitalName" value="Apollo Hospitals"/>
		<property name="departments">			
			<set>			
				<value>Reception</value>
				<value>Pharmacy</value>
				<value>Operation Theatre</value>
				<value>Test Centre</value>
				<value>Customer Care</value>
				<value>Reception</value>
			</set>
		</property>
	</bean>

</beans>
