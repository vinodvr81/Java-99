<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	
	<context:annotation-config/>
	<bean id="bmw" class="co.edureka.spring.core.beans.Car">
		<property name="model" value="B M W"/>
		<property name="cost" value="5700000"/>
	</bean>

	<bean id="sanjay" class="co.edureka.spring.core.beans.Customer">
		<property name="customerName" value="Sanjay Patil"/>		
	</bean>
	
	<bean id="audi" class="co.edureka.spring.core.beans.Car" primary="true">
		<property name="model" value="Audi"/>
		<property name="cost" value="6875000"/>
	</bean>	
</beans>
